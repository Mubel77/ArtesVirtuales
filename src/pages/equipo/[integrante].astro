---
import BasicLayout from '@/layouts/BasicLayout.astro';
import EquipoSection from '@/components/equipo'
import data from '@data'

export function getStaticPaths() {
  const equipo = data.nuestroEquipo.equipo.map((elemento)=>{
    let urlNombre = elemento.nombreUrl
    return {params: {integrante: urlNombre}}
  })
  return equipo 
}
const { integrante } = Astro.params;

const usuarioUrl = data.nuestroEquipo.equipo.find(user => user.nombreUrl === integrante);

//el client:idle es un configuracion de lectura para cierto archivos para react//
---
<BasicLayout title="Artes Virtuales" description="">
  <EquipoSection client:load data={usuarioUrl}/>
</BasicLayout>


